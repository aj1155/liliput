{
  "name": "llpt-dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run server && npm run watch",
    "server": "npm run server:local",
    "server:local": "NODE_ENV=local webpack-dev-server --config client-config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:local:was": "NODE_ENV=local-server webpack-dev-server --config client-config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:dev": "NODE_ENV=dev webpack-dev-server --config client-config/webpack.dev.js --progress --profile --watch --content-base src/",
    "clean": "rm -rf node_modules ./client-config/coverage dist && npm cache clean",
    "clean:dist": "rm -rf ./dist",
    "clean:install": "npm run clean && npm install",
    "ngc": "./node_modules/.bin/ngc -p ./tsconfig.aot.json",
    "build": "npm run build:local",
    "build:local": "NODE_ENV=local node ./node_modules/.bin/webpack --config client-config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "build:local:server": "NODE_ENV=local-server node ./node_modules/.bin/webpack --config client-config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "build:dev": "NODE_ENV=dev node ./node_modules/.bin/webpack --config client-config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "build:alpha": "NODE_ENV=alpha node ./node_modules/.bin/webpack --config client-config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "build:prod": "NODE_ENV=prod node ./node_modules/.bin/webpack --config client-config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "build:aot": "NODE_ENV=prod node ./node_modules/.bin/webpack --config client-config/webpack.aot.js --progress --profile --colors --display-error-details --display-cached --hide-modules",
    "webdriver-manager": "./node_modules/.bin/webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "test:e2e": "./node_modules/.bin/protractor ./client-config/protractor.conf.js",
    "test:e2e:live": "npm run e2e -- --elementexplorer",
    "test:unit": "./node_modules/.bin/karma start ./client-config/karma.conf.js",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "author": "liliput",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^4.0.0-beta.1",
    "@angular/compiler": "^4.0.0-beta.1",
    "@angular/compiler-cli": "^4.0.0-beta.1",
    "@angular/core": "^4.0.0-beta.1",
    "@angular/forms": "^4.0.0-beta.1",
    "@angular/http": "^4.0.0-beta.1",
    "@angular/platform-browser": "^4.0.0-beta.1",
    "@angular/platform-browser-dynamic": "^4.0.0-beta.1",
    "@angular/platform-server": "^4.0.0-beta.1",
    "@angular/router": "^4.0.0-beta.1",
    "@angular/upgrade": "^4.0.0-beta.1",
    "angular-in-memory-web-api": "^0.2.3",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.2",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.2.1",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.53",
    "@types/protractor": "^4.0.0",
    "@types/selenium-webdriver": "^2.53.38",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "codelyzer": "^2.0.0-beta.4",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "node-sass": "^4.1.1",
    "protractor": "^4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "selenium-webdriver": "^3.0.1",
    "source-map-loader": "^0.1.5",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^1.3.3",
    "ts-node": "^1.7.2",
    "tslint": "^4.2.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^2.0.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
